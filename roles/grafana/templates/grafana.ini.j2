[server]
root_url = http://{{hostvars[groups['grafana_servers'][0]]['ansible_default_ipv4']['address']}}:{{hostvars[groups['grafana_servers'][0]]['ansible_port'] | int + 58}}/grafana/
serve_from_sub_path = true

[security]
admin_user = {{ grafana_admin_user }}
admin_password = {{ grafana_admin_password }}